import java.util.ArrayList;


public class Main {

	public static void main(String[] args) {
		
		String str = "Вместе с главой Центральной комиссии по выборам и проведению республиканских референдумов Лидией Ермошиной мы подвели итоги прошедших выборов. Она рассказала о своих претензиях к участковым комиссиям, отдельным районам, где хотели запретить практически любые встречи кандидатов с избирателями, наблюдателям, а также намекнула на то, кто может занять ее кресло после парламентских выборов.";

		String[] slova = str.split("[,;:.!?\\s]+");
		for (int i = 0; i<= slova.length; i++){
			System.out.println(slova[i].toString());
		}

		

		
		
		
		
		
		

	}

}
