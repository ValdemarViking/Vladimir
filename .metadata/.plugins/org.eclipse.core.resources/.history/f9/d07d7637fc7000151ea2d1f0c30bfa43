
public class Main {

	public static void main(String[] args) {
		

		Thread t = new Thread(new Runnable() {
			public  void run() {
				
				for (int i =0; i<6; i++){
					System.out.println("новый поток" + i);
					
				}
				
			}
			
		});
		
		Thread t2 = new Thread(new Runnable() {
			public  void run() {
				System.out.println("новый поток 2");
			}
			
		});
		t.start();
		t2.start();
		System.out.println("главный поток!!!");
		
	}

}
