
public class Main {
	
	MyThread myThread1 = new MyThread();
	
	myThread1.setMain(main);
	
	MyThread myThread2 = new MyThread();
	myThread2.setMain(main);
	
	myThread1.start();
	myThread2.start();
	
	
	
	public void print10(){
		
		synchronized (this){
			
		System.out.println("thread name = " + Thread.currentThread().getName());
	
		for (int i=0; i<10; i++){
			
			System.out.println(i + " ");
			}
		}
	
	
	

	}

}
//String name